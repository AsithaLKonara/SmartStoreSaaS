# Database
DATABASE_URL="postgresql://username:password@localhost:5432/smartstore_ai"

# NextAuth.js
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-nextauth-secret-key-here"

# OAuth Providers
GOOGLE_CLIENT_ID="your-google-client-id"
GOOGLE_CLIENT_SECRET="your-google-client-secret"

# Payment Gateways
STRIPE_PUBLISHABLE_KEY="pk_test_your-stripe-publishable-key"
STRIPE_SECRET_KEY="sk_test_your-stripe-secret-key"
PAYHERE_MERCHANT_ID="your-payhere-merchant-id"
PAYHERE_SECRET_KEY="your-payhere-secret-key"

# WhatsApp Business API
TWILIO_ACCOUNT_SID="your-twilio-account-sid"
TWILIO_AUTH_TOKEN="your-twilio-auth-token"
TWILIO_PHONE_NUMBER="+1234567890"

# WhatsApp Web.js (Alternative)
WHATSAPP_SESSION_PATH="./sessions"

# AI Services
OPENAI_API_KEY="your-openai-api-key"
OLLAMA_BASE_URL="http://localhost:11434"

# Email Service
SMTP_HOST="smtp.gmail.com"
SMTP_PORT="587"
SMTP_USER="your-email@gmail.com"
SMTP_PASS="your-email-password"

# File Storage
CLOUDINARY_CLOUD_NAME="your-cloudinary-cloud-name"
CLOUDINARY_API_KEY="your-cloudinary-api-key"
CLOUDINARY_API_SECRET="your-cloudinary-api-secret"

# Redis (for caching and queues)
REDIS_URL="redis://localhost:6379"

# Courier APIs
PICKME_API_KEY="your-pickme-api-key"
PICKME_API_SECRET="your-pickme-api-secret"
ARAMEX_API_KEY="your-aramex-api-key"
ARAMEX_API_SECRET="your-aramex-api-secret"

# Social Media APIs
FACEBOOK_APP_ID="your-facebook-app-id"
FACEBOOK_APP_SECRET="your-facebook-app-secret"
INSTAGRAM_APP_ID="your-instagram-app-id"
INSTAGRAM_APP_SECRET="your-instagram-app-secret"

# Security
JWT_SECRET="your-jwt-secret-key"
ENCRYPTION_KEY="your-encryption-key"

# App Configuration
NODE_ENV="development"
APP_URL="http://localhost:3000"
API_URL="http://localhost:3000/api"

# Feature Flags
ENABLE_AI_CHATBOT="true"
ENABLE_WHATSAPP_INTEGRATION="true"
ENABLE_FACEBOOK_INTEGRATION="true"
ENABLE_PAYMENT_GATEWAYS="true"
ENABLE_COURIER_SYNC="true" 